    while(1){
        row=col=i=j=0;
        scanf("%d%d",&row,&col);
        if(!row&&!col) break;
        for(i=0;i<row;i++)
            for(j=0;j<col;j++){
                left[i][j]=up[i][j]=0; 
                scanf("%d",&y[i][j]);
        for(i=0;i<row;i++)
            for(j=0;j<col;j++)
                scanf("%d",&b[i][j]);
         
