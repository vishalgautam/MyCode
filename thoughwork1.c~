#include<stdio.h>
#include<string.h>
using namespace std;
int main(){
    int tc,i,vowel;
    scanf("%d",&tc);
    char brow[100],vow[26],c='a';
    memset(vow,0,26);
    vow[0]=1;vow['e'-c]=1;vow['i'-c]=1;
    vow['o'-c]=1;vow['u'-c]=1;
    while(tc--){
        scanf("%d",&brow);
        i=4;vowel=0;
        while((c=brow[i])!='.'){
            if(vow[c-'a']==1){
                vowel++;
            }
            i++;
        }
        printf("%d/%d",
        cout<<(i-4)-vowel+4<<"/"<<i+4<<endl;
    }
    return 0;
}


