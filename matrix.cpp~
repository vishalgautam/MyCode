#include<iostream>
#include<climits>
#include<cstring>
using namespace std;
int main(){
	int row[6],col[6],count=6;
	int matrix[7][7];
	int i=1,sum,j,k,c,cur;
	while(i<=count){
		cin>>row[i]>>col[i];
		i++;
	}
	memset(matrix,0,(count+1)*(count+1)*sizeof(int));
	for(j=2;j<=count;j++){
		matrix[j-1][j-1]=matrix[j][j]=0;
		for(i=j-1;i>0;i--){
			sum=INT_MAX;
			for(k=i;k<j;k++){
				cout<<i<<" "<<k<<" "<<j<<"\n";
				cur=matrix[i][k]+matrix[k+1][j]+row[i]*col[k]*col[j];
				sum=sum>cur?cur:sum;
			}
			matrix[i][j]=sum;
		}
	}
	for(i=1;i<=count;i++){
		for(j=1;j<=count;j++)
			cout<<matrix[i][j]<<" ";
		cout<<endl;
	}
	cout<<"Minimum operation :"<<matrix[1][count]<<endl;
}



